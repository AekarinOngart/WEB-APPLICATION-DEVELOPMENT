    <script>
        function BtnClick() {
            $.ajax({
                url: "/PickupOrder/GetAll",
                method: "GET",
                success: function (data) {
                    console.log(data.value)
                    for (i in data.value){
                    console.log(i)
                    MM = data.value[i]
                    console.log(MM)
                    $(`#Name${i}`).text(MM.name);
                    $(`#EndTime${i}`).text(MM.endTime);
                    }

                },
                error: function (err) {
                    console.error(err);
                }

            })
        
}
