    <script src="~/js/effect.js"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script>
        function BtnClick() {
            $.ajax({
                url: "/PickupOrder/GetAll",
                method: "GET",
                success: function (data) {
                    console.log(data.value)
                    for (var i in data.value){ 
                        if (i == 0) {
                            document.getElementById("one").style.visibility = "visible";
                        }
                        else if (i == 1 ) {
                            document.getElementById("two").style.visibility = "visible";
                        }
                        else if (i == 2) {
                            document.getElementById("three").style.visibility = "visible";
                        }
                        else if (i == 3) {
                            document.getElementById("four").style.visibility = "visible";
                        }
                        else if (i == 4) {
                            document.getElementById("five").style.visibility = "visible";
                        }
                        else if (i == 5) {
                            document.getElementById("six").style.visibility = "visible";
                        }
                        else if (i == 6) {
                            document.getElementById("seven").style.visibility = "visible";
                        }
                        else if (i == 7) {
                            document.getElementById("eight").style.visibility = "visible";
                        }
                    console.log(i)
                    MM = data.value[i]
                    console.log(MM)
                    $(`#Name${i}`).text(MM.name);
                    var text = MM.price+" บาท"
                    $(`#EndTime${i}`).text(text);
                        
                    }
                    

                },
                error: function (err) {
                    console.error(err);
                }

            })
            
            
}
            function ssorder0(){
                sessionStorage.setItem("Ssorder", 0);
                location.href = 'https://localhost:7110/home/addorder'

            }
